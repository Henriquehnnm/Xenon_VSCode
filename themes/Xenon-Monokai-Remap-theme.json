  "tokenColors": [
    {
      "scope": "comment",
      "settings": { "foreground": "#8f8f9b", "fontStyle": "italic" }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": { "foreground": "#ff7b8f" }
    },
    {
      "scope": "keyword.operator",
      "settings": { "foreground": "#ff7b8f" }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": { "foreground": "#67f1d2" }
    },
    {
      "scope": "entity.name.tag",
      "settings": { "foreground": "#ff7b8f" }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": { "foreground": "#ffcc91" }
    },
    {
      "scope": "string",
      "settings": { "foreground": "#ffcc91" }
    },
    {
      "scope": "constant.numeric",
      "settings": { "foreground": "#bea5ff" }
    },
    {
      "scope": "constant.language.boolean",
      "settings": { "foreground": "#bea5ff" }
    },
    {
      "scope": "constant",
      "settings": { "foreground": "#bea5ff" }
    },
    {
      "scope": ["entity.name.type", "support.type"],
      "settings": { "foreground": "#d3fa83" }
    },
    {
      "scope": "entity.name.function.constructor",
      "settings": { "foreground": "#d3fa83" }
    },
    {
      "scope": ["variable", "support.variable"],
      "settings": { "foreground": "#e8e8ef" }
    },
    {
      "scope": "variable.special",
      "settings": { "foreground": "#ffcc91" }
    },
    {
      "scope": ["support.type.property-name", "meta.object-literal.key"],
      "settings": { "foreground": "#e8e8ef" }
    },
    {
      "scope": "punctuation",
      "settings": { "foreground": "#e8e8ef" }
    }
  ]
}
